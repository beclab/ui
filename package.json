{
  "name": "@bytetrade/ui",
  "version": "0.1.12",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "copy-folder": "cp -R src/styles lib/",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md,mdx}\"",
    "lib": "vue-cli-service build --target lib --name bytetrade-ui --dest lib packages/index.ts && tsc --emitDeclarationOnly -p tsconfig.json && npm run copy-folder",
    "posttest": "npm run format",
    "prepare": "husky install",
    "build-dev": "npm run lib && yalc push"
  },
  "main": "lib/bytetrade-ui.common.js",
  "browser": {
    "crypto": false
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@quasar/extras": "^1.16.7",
    "axios": "^1.3.4",
    "core-js": "^3.8.3",
    "crypto-browserify": "^3.12.0",
    "moment": "^2.29.4",
    "multiformats": "9.6.4",
    "quasar": "^2.6.0",
    "vue": "^3.2.13",
    "vue-cropper": "^1.0.8",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-typescript": "^9.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.3",
    "less": "^4.0.0",
    "less-loader": "^8.0.0",
    "lint-staged": "^15.0.1",
    "prettier": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "1.32.12",
    "sass-loader": "^12.0.0",
    "typescript": "~4.5.5",
    "vue-cli-plugin-quasar": "5.1.0",
    "vue-loader": "^17.3.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "types": "lib/index.d.ts"
}
